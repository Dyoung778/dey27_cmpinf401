package edu.pitt.dey27;

import javax.swing.JOptionPane;

public class pythagoreanTheorem {

	public static void main(String[] args) {
		double a = 0;
		double b = 0;
		double hypotenuse = Math.sqrt(Math.pow(a,a )+ Math.pow(b,b));
	JOptionPane.showInputDialog("Enter a number for side a");
	JOptionPane.showInputDialog("Enter a number for side b");
    JOptionPane.showMessageDialog(null, "The hypotenuse is " + hypotenuse);
	}
 
}
